
struct unique_int
{
    val: i64;

    fn copy = delete;
    fn assign = delete;
    fn drop(self: &unique_int)
    {
        println("dropping");
        println(self->val);
    }
}

fn add(x: i64, y: i64) -> i64
{
    return x + y;
}

fn myfunc(x: unique_int)
{
    println(x.val);
}

{
    x := unique_int(5);
    #myfunc(x);

    println("done");
    println(add(3, 4));
    a := 5;
    b := 6;
    println(add(a, b));
}

println("after");