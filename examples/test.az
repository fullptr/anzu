
struct unique_int
{
    val: i64;

    fn copy(self: &unique_int) -> unique_int
    {
        return unique_int(self->val);
    }

    fn assign(self: &unique_int, other: &unique_int)
    {
        other->val = self->val;
    }

    fn drop(self: &unique_int)
    {
        println("dropping");
    }
}

fn add(x: i64, y: i64) -> i64
{
    return x + y;
}

fn myfunc(x: unique_int)
{
    println(x.val);
}

{
    x := [unique_int(5), unique_int(6)];
    y := x;
    y = x;
    println(y[0u].val);
}