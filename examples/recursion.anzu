# fibbonacci to demonstrate functions and recursion

function print 1 0
    . "\n" .
end

function fibb 1 1
    if dup 0 == do
        0 return
    elif dup 1 == do
        1 return
    end

    dup 1 - -> a
    dup 2 - -> b
    a fibb b fibb +
end

0 while dup 10 < do
    dup fibb print
    1 +
end